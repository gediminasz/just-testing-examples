{
    "justTesting.baseCommand": "pipenv run python manage.py test",
    "justTesting.runFileCommand": "{base} {module}",
    "justTesting.runOnCursorCommand": "{base} {module}.{className}.{testName}",
    "justTesting.expressions": {
        "className": {
            "regex": "class (.+TestCase)\\("
        }
    }
}
