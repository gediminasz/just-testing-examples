{
    "[python]": {
        "justTesting.baseCommand": "python -m pytest -v",
        "justTesting.runAllCommand": "{base}",
    },
    "[javascript]": {
        "justTesting.baseCommand": "jest --config=./javascript/jest.config.js",
        "justTesting.runAllCommand": "{base}",
        "justTesting.runOnCursorRegex": "test\\((.+),",
        "justTesting.runOnCursorCommand": "{base} -t {testName}"
    }
}
